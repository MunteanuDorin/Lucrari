package com.company;
//Clasa parinte pentru CarteFilosofie
class Carte {
    private String denumirea;
    private String autorul;
    private float pretul;
    private double pagini;
    private String editura;
    Carte(String denumirea,String autorul,float pretul,double pagini, String editura){
        this.denumirea=denumirea;
        this.autorul=autorul;
        this.pretul=pretul;
        this.pagini=pagini;
        this.editura=editura;
    }
    private float getDenumirea() {
        return this.denumirea;
    }
    private String getAutorul() {
        return this.autorul;
    }
    private String getPretul() {
        return this.pretul;
    }
    private int getPagini() {
        return this.pagini;
    }
    private int getEditura() {
        return this.editura;
    }
    private void setDenumirea(String denumirea) {
        this.denumirea=denumirea;
    }
    private void setAutorul(String autorul) {
        this.autorul=autorul;
    }
    private void setPretul(float pretul) {
        this.pretul=pretul;
    }
    private void setPagini(double pagini) { 
        this.pagini=pagini;
    }
    private void setEditura(String editura) { 
        this.editura=editura;
    }
}
